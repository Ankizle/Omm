import ~ '?~utils/len.omm';
import ~ '?~utils/startswith.omm';

proc ~ haschar(main, inc) {

  if (len(inc) != 1) return ~ false;

  each (main) {
    if (v = inc) return ~ true;
  }

  return ~ false;
}

proc ~ has(main, inc) {

  if (len(inc) > len(main)) return ~ false;

  each (main) {

    if (startswith(main, inc)) return ~ true;

    main: 1 - main;
  }

  return ~ false;
}
