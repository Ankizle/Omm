proc ~ tostring(val) {
  return ~ '' + val;
}

proc ~ Math.floor(val) {
  return ~ val - (val % 1);
}

proc ~ Math.ceil(val) {
  return ~ val + (val % 1);
}

proc ~ isPrime(val) {
  cur: val - 1;

  loop (cur > 1) {

    if (val % cur = 0) {
      return ~ false;
    }

    cur--;
  }

  return ~ true;
}
