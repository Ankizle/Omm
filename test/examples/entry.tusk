//link the printf function (from glibc) to this tusk program
//#i8 is the equivalent of char* in C
//i32 is the equivalent of int in C (32-bit signed integer)
link fn printf(#i8, i32);

pub stat fn main() {
    printf("%d\n", examples.fib.fib(10)); //call the fib (file: fib.tusk) function with `10` and print the result (outputs 55)
    printf("%d\n", examples.curryfunc.curriedfn()); //outputs 130
    printf("%d\n", examples.purefunc.purefn()); //outputs 45
    printf("%d\n", examples.objs.dostuff()); //outputs 10
};